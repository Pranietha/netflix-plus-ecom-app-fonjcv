{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["standard", "plugin:react/recommended"],
  "parseOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
    "rules": {
    "semi": "off",
    "react/react-in-jsx-scope": "off",
    "indent": ["error", 2]
  }
}
